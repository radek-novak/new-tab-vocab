{
  "name": "New Tab Vocab",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "chrome extension for language learning",
  "homepage_url": "https://github.com/radek-novak/new-tab-vocab",
  "chrome_url_overrides": {
    "newtab": "override/override.html"
  },
  "icons": {
     "128": "icon.png"
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": false,
    "open_in_tab": false
  },
  "permissions":["storage"]
}
